# Supabase Self-hosted Configuration
# للجهات الحكومية السيادية
# ====================================

# Database
POSTGRES_PASSWORD=YourSuperSecretPasswordHere123!
POSTGRES_DB=postgres
POSTGRES_PORT=5432

# JWT Secret (يجب أن يكون 32 حرف على الأقل)
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

# API Keys (استخدم supabase CLI لتوليدهم)
ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# URLs
API_EXTERNAL_URL=http://your-server-ip:8000
SITE_URL=http://your-frontend-domain.com
ADDITIONAL_REDIRECT_URLS=

# Auth Settings
DISABLE_SIGNUP=false
JWT_EXPIRY=3600
JWT_AUD=authenticated
JWT_DEFAULT_GROUP_NAME=authenticated

# Email/SMTP (اختياري - للـ OTP والإيميلات)
SMTP_ADMIN_EMAIL=admin@your-domain.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
SMTP_SENDER_NAME=Sudanese Consulate

# Studio (Admin Panel)
STUDIO_PORT=3000

# Storage
FILE_SIZE_LIMIT=52428800
STORAGE_BACKEND=file

# ====================================
# Frontend Environment Variables
# ====================================

# استخدم هذه القيم في ملف .env للمشروع
VITE_SUPABASE_URL=http://your-server-ip:8000
VITE_SUPABASE_ANON_KEY=same-as-ANON_KEY-above
